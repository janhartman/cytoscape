<!DOCTYPE html>
<html>
  <head>
    <title>Cytoscape demo</title>
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1">
    <script src="https://code.jquery.com/jquery-2.0.3.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/cytoscape/2.7.13/cytoscape.min.js"></script>

    <style>
      body {
        font-family: helvetica;
        font-size: 14px;
      }
      #cy {
        width: 100%;
        height: 100%;
        position: absolute;
        left: 0;
        top: 0;
        z-index: 999;
      }
      h1 {
        opacity: 0.5;
        font-size: 1em;
      }
    </style>

  <script>
    $(function(){
      $.getJSON('data', function(data) {
        
        var cy = window.cy = cytoscape({
          container: $('#cy'),
          layout: {
            name: 'cose',
            idealEdgeLength: 100,
            nodeOverlap: 20
          },
          
          style: data.style,
          
          elements: data.elements
        });
      });
    });
  </script>

  </head>

  <body>
    <h1>Cytoscape demo</h1>

    <div id="cy"></div>
    <%  
      
    %>

  </body>

</html>